package com.exam1;

 abstract class Abc {


	
	public abstract void disp1() ;

	

	public void disp2() {
		System.out.println("2");
	}
}

class B extends Abc {
	

	
	public void disp1() {
		System.out.println("3");
	}

	public void disp3() {
		System.out.println("4");
	}

}

public class Upcasting {

	public static void main(String args[]) {
//	B obj = new B();
		
