package com.java1;




class A2{
	private int a;
	private int b;
	public A2() {
		
	}
	public int getA() {
		return a;
	}
	public void setA(int a) {
		this.a = a;
	}
	public int getB() {
		return b;
	}
	public void setB(int b) {
		this.b = b;
	}
	
	
}
public class P2 {

	public static void main(String[] args) {
		A2 obj=new A2();
		obj.setA(1);
		obj.setB(2);
	}

}
