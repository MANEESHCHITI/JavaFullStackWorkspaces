package com.exam1;

class Abc {

	public Abc() {
		
	}
	public void disp1() {

		System.out.println("1");
	}

	public void disp2() {
		System.out.println("2");
	}
}

class B extends A {
	public void disp1() {
		System.out.println("3");
	}

	public void disp3() {
		System.out.println("4");
	}

}

public class Upcasting {

	public static void main(String args[]) {
		A obj = new B();
		obj.disp1();
		obj.disp2();
		obj.disp3();
	}
}
