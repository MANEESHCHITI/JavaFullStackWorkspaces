package com.java1;

public class Primee {

	public static void main(String[] args) {

		int num=11;
		boolean isPrime=true;
		for(int i=2;i<=11;i++) {
			if(num%i==0) {
				isPrime=false;
				break;
		}
	}
		if(isPrime) {
			System.out.println(num+"is a prime nuber");
		}
		else {
			System.out.println(num+"is not a prime number");
		}

}
}
