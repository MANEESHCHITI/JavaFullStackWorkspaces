package PackInherit1;

public class HierachialInheritance {

	public static void main(String[] args) {
	
 Parent11 obj=new Parent11();
 Child11 obj1=new Child11();
 Child2 obj2=new Child2();
 Child3 obj3=new Child3();
 obj.disp();
 obj1.disp();
 obj2.disp();
 obj3.disp();
	}

}
class Parent11
{
	public void disp()
	{
		System.out.println("parent");
	}
}
class Child11 extends Parent11
{
	public void disp()
	{
		System.out.println("child1");
	}
}
class Child2 extends Parent11
{
	public void disp()
	{
		System.out.println("child2");
	}
}
class Child3 extends Parent11
{
	public void disp()
	{
		System.out.println("child3");
	}
}

