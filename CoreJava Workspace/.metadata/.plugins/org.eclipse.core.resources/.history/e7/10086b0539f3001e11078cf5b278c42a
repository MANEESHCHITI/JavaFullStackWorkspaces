package Demo1;

import java.util.Scanner;

class Ex1
{
	int a;
	int b;
	int c;
}



public class ScannerForInstance {

	public static void main(String[] args) {
		

	
Scanner s=new Scanner(System.in);
Ex1 obj=new Ex1();
obj.a=s.nextInt();
obj.b=s.nextInt();
obj.c=s.nextInt();
System.out.println(obj.a);
System.out.println(obj.b);
System.out.println(obj.c);

}
}