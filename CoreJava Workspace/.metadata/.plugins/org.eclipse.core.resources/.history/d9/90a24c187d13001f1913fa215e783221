package PackInherit1;

public class SingleLevelInheritance {

	public static void main(String[] args) {


	}

}
class A
{
	public void disp1()
	{                                           // single level inheritance overriding
	System.out.println("parent");
	}
}
class B extends A
{
public void disp1()
{
	System.out.println("child");
	}
}
