package array;

public class Array4 {

	public static void main(String[] args) {
		A2 obj=new A2();
		obj.disp();

	}

}
class A2
{	
	int a[]=new int[] {3,2,1,9,6,12,7};
	
	

	public void disp()
	{
		for(int i=0;i<a.length;i++)
		{int count=0;
			for(int j=0;j<a.length;j++)
			{
				if(a[i]>=a[j])
				{
					count++;
					if(count==7)                          
					{
						System.out.println(a[i]);
					}
				}
				else
				{
					break;
				}
			}
			
			
			
			
		}
	
	}
	public void dispp()
	{
		for(int i=0;i<a.length;i++)
		{int count=0;
			for(int j=0;j<a.length;j++)
			{
				if(a[i]>=a[j])
				{
					count++;
					if(count==7)                          
					{
						System.out.println(a[i]);
					}
				}
				else
				{
					break;
				}
			}
			
			
			
			
		}
	
	}
}
