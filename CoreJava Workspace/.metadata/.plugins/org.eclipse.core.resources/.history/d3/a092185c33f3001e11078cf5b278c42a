package Demo1;
import java.util.Scanner;

class A
{
	public void show(int a)
	{
		for(int i=1;i<=a;i++)
		{
			System.out.println(i);
		}
	}
}



public class ScannerExample {

	public static void main(String[] args) {
		
Scanner s=new Scanner(System.in);
int value=nextInt();
A obj=new A();
obj.show(value);

	}

}
