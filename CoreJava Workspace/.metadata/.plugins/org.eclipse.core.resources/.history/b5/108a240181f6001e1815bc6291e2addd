package caseStudys;

class A5
{
	public void disp()
	{
	
		for(int i=1;i<=7;i++)
		{int mul=1;
			for(int j=1;i<=i;j++)
			{ 
				mul=mul*j;
			}
			System.out.println(mul);
			
			
		}
		
			
	}
}

	





















public class Factorial {

	public static void main(String[] args) {
		
A5 obj=new A5();
obj.disp();
	}

}
