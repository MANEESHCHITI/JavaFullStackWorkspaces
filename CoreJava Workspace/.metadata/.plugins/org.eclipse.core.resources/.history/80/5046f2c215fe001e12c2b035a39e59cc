package array;

public class Array8 {

	public static void main(String[] args) {
		Asc obj=new Asc();
		obj.ascending();

	}

}
class Asc
{
	int temp;
	int a[]=new int[] {8,2,6,1};
	public void ascending()                    // array in ascending and decending.
	{
		for(int i=0;i<a.length;i++)
		{
			for(int j=i+1;j<a.length;j++)
			{
				if(a[i]>a[j])
				{
					temp=a[i];
					a[i]=a[j];
					a[j]=temp;
					
					
					
				}
			}
			System.out.println(a[i]);
		}
	
	//for(int c:a)
	//{
	//	System.out.println(c);
	//}
	}
	}
